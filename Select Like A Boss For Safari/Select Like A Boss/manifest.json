{
  "manifest_version": 2,
  "name": "Select Like A Boss",
  "description": "Select text inside links.",
  "version": "1.2",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_title": "Select Like A Boss",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
